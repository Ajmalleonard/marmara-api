# Database
DATABASE_URL="your_database_url_here"

# JWT
JWT_SECRET="your-jwt-secret-key"

# Stripe Payment Configuration
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key"

# PesaPal Payment Configuration
PESAPAL_CONSUMER_KEY="your_pesapal_consumer_key"
PESAPAL_CONSUMER_SECRET="your_pesapal_consumer_secret"
PESAPAL_DEBUG=true
PESAPAL_CALLBACK_URL="http://localhost:3000/payment/callback"
PESAPAL_NOTIFICATION_ID="your_pesapal_notification_id"

# Amadeus API Configuration
AMADEUS_CLIENT_ID="your_amadeus_client_id"
AMADEUS_CLIENT_SECRET="your_amadeus_client_secret"
AMADEUS_HOSTNAME="test.api.amadeus.com"

# WhatsApp Configuration
# Comma-separated list of admin phone numbers (with country code, no + sign)
# Example: 905551234567,905559876543
ADMIN_WHATSAPP_NUMBERS="255710097350"

# Admin dashboard URL for notifications
ADMIN_DASHBOARD_URL="https://your-admin-dashboard.com"

# Email Configuration
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# Redis Configuration (for caching)
REDIS_HOST="localhost"
REDIS_PORT=6379
REDIS_PASSWORD=""

# Application Configuration
PORT=3000
NODE_ENV="development"

